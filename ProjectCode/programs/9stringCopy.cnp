char[] convertToCharacterArray( string str )
{
	char arr[len(str)];

	for( int i = 0 ; i < len(str) ; i++ )
	{
		arr[i] = str[i];
	}
	
	return arr;
}


void main()
{
	print("Enter the string");
	string s;
	s = scan();

	char arr[] = convertToCharacterArray( s );

	print("The character array is");
	for( int i = 0 ; i < len(s) ; i++ )
	{
		prints(arr[i]);
	}
	print();
}
