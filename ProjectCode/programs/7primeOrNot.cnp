bool primeOrNot( int n )
{
	bool b = true;
	for( int i = 2 ; i <= (n/2) ; i++ )
	{
		if( n % i == 0 )
		{
			b = false;
			break;
		}
	}

	return b;
}

void main()
{
	int n;
	print("Enter the Number");
	n = scan();

	bool b = primeOrNot(n);

	if( b ) 
	{
		print(n, " is a prime Number");
	}
	else
	{
		print(n, " not a prime number");
	}
}
